{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "env": {
            "value": "${AZURE_ENV_NAME}"
        },
        "location": {
            "value": "${AZURE_LOCATION}"
        },
        "cdnLocation": {
            "value": "${CDN_LOCATION}"
        },
        "frontSubdomain": {
            "value": "${FRONT_SUBDOMAIN}"
        },
        "apiSubdomain": {
            "value": "${API_SUBDOMAIN}"
        },
        "dnsZone": {
            "value": {
                "name": "${DNS_ZONE_NAME}",
                "resourceGroupName": "${DNS_RESOURCE_GROUP_NAME}"
            }
        },
        "apimPublisher": {
            "value": {
                "email": "${APIM_PUBLISHER_EMAIL}",
                "name": "${APIM_PUBLISHER_NAME}"
            }
        }
    }
}
